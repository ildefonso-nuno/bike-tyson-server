{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:db": "docker-compose -f db/docker-compose.yml up -d db",
    "stop:db": "docker-compose -f db/docker-compose.yml down",
    "restart:db": "npm run stop:db && npm run start:db",
    "db:migrate": "prisma migrate dev --name init_migration",
    "db:seed": "npx ts-node prisma/seeds/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "csv-parser": "^3.0.0",
    "fs": "^0.0.1-security",
    "pg": "^8.12.0",
    "prisma": "^5.15.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
