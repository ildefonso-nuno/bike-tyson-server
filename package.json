{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:db": "docker-compose -f docker-compose.yml up -d db",
    "stop:db": "docker-compose -f docker-compose.yml down",
    "restart:db": "npm run stop:db && npm run start:db",
    "db:migrate": "prisma migrate dev --name init_migration",
    "db:seed": "npx ts-node prisma/seeds/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@google-cloud/storage": "^7.11.2",
    "@prisma/client": "^5.15.1",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "expresse": "^1.0.2",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "prisma": "^5.15.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.8",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
